    "# https://www.acmicpc.net/problem/2012\n",
    "input = open('input.txt').readline\n",
    "\n",
    "expected = []\n",
    "for _ in range(int(input())):\n",
    "    expected.append(int(input()))\n",
    "    \n",
    "result = 0\n",
    "expected = sorted(expected)\n",
    "for i in range(len(expected)):\n",
    "    result += abs((i+1)-expected[i])\n",
    "    \n",
    "print(result)"
   ]
