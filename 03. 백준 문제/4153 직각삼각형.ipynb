    "# a, b, c를 비교해 가장 큰걸 찾고 max**2 == m**2 + n**2 하기 \n",
    "# vs\n",
    "# 제곱 다 해놓고 아래처럼 세번 더해보기\n",
    "\n",
    "while True:\n",
    "    a, b, c = map(int, input().split())\n",
    "    if a==0:\n",
    "        break\n",
    "    a *= a\n",
    "    b *= b\n",
    "    c *= c\n",
    "    if a+b == c or a+c==b or c+b==a :\n",
    "        print('right')\n",
    "    else:\n",
    "        print('wrong')"
