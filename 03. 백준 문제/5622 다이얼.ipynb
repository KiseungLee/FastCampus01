   "source": [
    "word = input()\n",
    "\n",
    "result = 0\n",
    "\n",
    "for c in word:\n",
    "    if ord(c)<=82:   # A ~ R \n",
    "        result += (ord(c)-65)//3 + 2 + 1\n",
    "    elif ord(c)==83: # S\n",
    "        result += 8\n",
    "    elif ord(c)<=86: # T, U, V\n",
    "        result += 9\n",
    "    else:            # W, X, Y, Z\n",
    "        result += 10\n",
    "\n",
    "print(result)"
   ]
