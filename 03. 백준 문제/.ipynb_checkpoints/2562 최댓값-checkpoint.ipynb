    "# 현재 최댓값의 인덱스, 값 변수 선언 및 초기화\n",
    "max_idx = 0\n",
    "max_num = 0\n",
    "\n",
    "# 입력 및 최댓값의 인덱스, 값 갱신\n",
    "for i in range(9):\n",
    "    num = int(input())\n",
    "    if num>max_num:\n",
    "        max_num = num\n",
    "        max_idx = i+1\n",
    "        \n",
    "# 출력\n",
    "print(max_num)\n",
    "print(max_idx)"
