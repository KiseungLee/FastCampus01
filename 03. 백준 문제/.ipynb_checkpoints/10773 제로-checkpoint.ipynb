   "source": [
    "# 파이썬엔 스택 자료구조 없음. 그냥 기본 배열을 스택으로 사용\n",
    "import sys\n",
    "stack = []\n",
    "input = sys.stdin.readline\n",
    "for _ in range(int(input())):\n",
    "    n = int(input())\n",
    "    if n==0:\n",
    "        stack.pop()\n",
    "    else:\n",
    "        stack.append(n)\n",
    "print(sum(stack))"
   ]
