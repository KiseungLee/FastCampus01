   "source": [
    "# 데이터 입력\n",
    "n = int(input())\n",
    "arr = list(map(int, input().split()))\n",
    "\n",
    "# 최소, 최대 변수 선언\n",
    "min_num = 1000000\n",
    "max_num = -1000000\n",
    "\n",
    "# 배열 돌면서 최대, 최소 값 찾기\n",
    "for num in arr:\n",
    "    if num>max_num:\n",
    "        max_num = num\n",
    "    elif num<min_num:\n",
    "        min_num = num\n",
    "\n",
    "# 출력\n",
    "print(min_num, max_num)"
   ]
