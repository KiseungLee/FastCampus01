    "# 입력\n",
    "n = int(input())\n",
    "\n",
    "# 메모이제이션 초기화\n",
    "arr = [0]*(n+1)\n",
    "if n!=0:\n",
    "    arr[1] = 1\n",
    "\n",
    "# 메모\n",
    "for i in range(2, n+1):\n",
    "    arr[i] = arr[i-1]+arr[i-2]\n",
    "    \n",
    "# 출력\n",
    "print(arr[n])"
   ]
